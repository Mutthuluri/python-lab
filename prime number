n = int(input("Enter the number to check prime number\n"))
count = 0
if(n<0):
    print("Entered  number is a negative number\nEnter a positive number")
else:
    for i in range(n+1):
        if n%2==0:
            count += 1
    if n == 1:
        print("Entered number is a prime number")
    elif count == 2:
        print("Entered number is a prime number")
    else:
        print("Entered number is a compositer number")
